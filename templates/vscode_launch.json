{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "WAPT: install",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "install",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "WAPT: remove",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "remove",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "WAPT: session-setup",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "session-setup",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "WAPT: -i build upload",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "-i",
                "build-upload",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "WAPT: uninstall",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "uninstall",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "WAPT: audit",
            "type": "python",
            "request": "launch",
            "program": "${config:python.wapt-get}",
            "args": [
                "audit",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal"
        }
    ]
}